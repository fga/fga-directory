base theme = 'omega'
version = '1.5'

;
; ----------------------------------
; Theme specific styles
; ----------------------------------
stylesheets[all][] = 'css/reset.css'
stylesheets[all][] = 'css/typography.css'
css[global.css][name] = 'global styles'

css[reset.css][name] = 'reset styles'
css[reset.css][description] = 'Reset styles for directory, alpha-reset was loading after typography.'
